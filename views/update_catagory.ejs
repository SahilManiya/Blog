<%- include('header')%>

<div class="main-content">
    <div class="section__content section__content--p30">
        <div class="container-fluid">
            <div class="row"> 
                <div class="col-md-12">
                    <div class="card"> 
                        <div class="card-body card-block">
                            <form action="/admin/catagory/editCatagoryData" method="post" class="form-horizontal">
                                <input type="hidden" name="EditCatagoryId" value="<%=CatagoryData.id%>">
                                <div class="row form-group">
                                    <div class="col col-md-3">
                                        <label for="text-input" class=" form-control-label">Enter Category</label>
                                    </div>
                                    <div class="col-12 col-md-9">
                                        <input type="text" id="text-input" name="catagory" placeholder="Enter Category" value="<%=CatagoryData.catagory%>" class="form-control">
                                    </div>
                                </div>
                               
                                <div class="card-footer">
                                    <button type="submit" class="btn btn-primary btn-sm">
                                        <i class="fa fa-dot-circle-o"></i> Edit
                                    </button>
                                    <button type="reset" class="btn btn-danger btn-sm">
                                        <i class="fa fa-ban"></i> Reset
                                    </button>
                                </div>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
            <%-include('footer')%>